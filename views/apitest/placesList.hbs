<!-- views/apitest/placesList.hbs -->
<div class='list-container'>
      <h2 class='places-title'>PLACES LIST</h2>
      {{#each venuesIDarray}}
      <form action="/apitest/new" method="POST">
            <button type="submit" value="hola">     
                  <div class='card'>
                        <img src={{this.imgUrl}}>
                        <div class="card-info">
                              <p>{{this.name}}</p>
                              <p>{{this.location.address}}</p>
                              <p>{{this.location.city}}</p>
                        </div>
                  </div>
            </button>

      </form>
      {{/each}}
</div>